<manifest xmlns:android="http://schemas.android.com/apk/res/android"

          package="com.example.kaitlynanderson.andevcon2017">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".BottomSheetActivity"/>
        <activity android:name=".CustomBottomSheetActivity"/>
        <activity android:name=".PercentRelativeLayoutActivity"/>
        <activity android:name=".FABCollapsingToolbarActivity"/>
        <activity android:name=".PercentToConstraintActivity"/>
        <activity android:name=".ConstraintPlaygroundActivity"/>
        <activity android:name=".ConstraintChainsActivity"/>
    </application>

</manifest>
